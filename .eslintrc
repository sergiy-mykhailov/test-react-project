{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "plugins": ["react", "import"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "arrow-body-style": 0,
    "max-len": 0,
    "no-console": 0,
    "no-shadow": 0,
    "no-script-url": 0,
    "no-useless-escape": 0,
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error"
  }
}